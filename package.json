{
  "name": "fannypack",
  "version": "4.12.5",
  "description": "An accessible, composable, and friendly React UI Kit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/fannypack.min.js",
  "types": "ts/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:lib && yarn build:types",
    "build:lib": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build:docs": "yarn docz:build && yarn playroom:build",
    "build:fab": "yarn build:docs && yarn fab:compile",
    "fab:compile": "fab-static docs/",
    "clean": "rimraf es/ lib/ dist/ ts/",
    "develop": "rollup -c -w",
    "prepublish": "yarn build",
    "postpublish": "yarn clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ --ext js,ts,tsx",
    "lint:fix": "eslint src/ --fix",
    "test": "yarn type-check && yarn lint && yarn jest",
    "jest": "jest",
    "docz:start": "docz dev",
    "docz:build": "docz build",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@types/classnames": "2.2.6",
    "@types/prop-types": "15.5.8",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/styled-components": "4.1.8",
    "airbnb-prop-types": "2.11.0",
    "classnames": "2.2.6",
    "conditional-wrap": "1.0.0",
    "focus-trap": "3.0.0",
    "lodash": "4.17.11",
    "markdown-react-js": "1.0.2",
    "polished": "2.3.0",
    "prop-types": "15.6.2",
    "react-loads": "6.0.3",
    "react-remarkable": "1.1.3",
    "react-syntax-highlighter": "8.0.1",
    "reakit": "0.16.0",
    "styled-selector": "0.1.1",
    "styled-tools": "1.5.2",
    "webfontloader": "1.6.28"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@emotion/core": "10.0.5",
    "@emotion/styled": "10.0.5",
    "@fab/static": "0.1.4",
    "@medipass/eslint-config-react-medipass": "8.3.7",
    "@reactions/component": "2.0.2",
    "@types/jest": "23.3.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "1.9.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-docz-fannypack": "0.13.3",
    "docz": "0.13.7",
    "docz-theme-default": "0.13.7",
    "emotion-theming": "10.0.5",
    "eslint": "5.9.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.14.0",
    "flow-bin": "0.83.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.3.1",
    "parse-prop-types": "0.3.0",
    "playroom": "0.7.2",
    "query-string": "6.2.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-test-renderer": "16.8.3",
    "react-testing-library": "5.3.1",
    "rimraf": "2.6.2",
    "rollup": "0.67.4",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-ignore": "1.0.5",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-proxy-directories": "1.2.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-terser": "4.0.3",
    "typescript": "3.3.3",
    "typescript-eslint-parser": "21.0.2"
  },
  "author": "Jake Moxey",
  "homepage": "https://github.com/fannypackui",
  "license": "MIT",
  "repository": "fannypackui/fannypack",
